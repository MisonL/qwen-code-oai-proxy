# Server configuration
PORT=8080
HOST=localhost

# Qwen API configuration
# You need to set this to your actual Qwen access token
# QWEN_ACCESS_TOKEN=your-qwen-access-token

# Debug logging configuration
# Set to true to enable debug logging (log files will be created)
DEBUG_LOG=false

# Maximum number of debug log files to keep (older files will be deleted)
LOG_FILE_LIMIT=20
# Server configuration
PORT=8080
HOST=localhost

# Debug logging configuration
# Set to true to enable debug logging (log files will be created)
DEBUG_LOG=false

# Maximum number of debug log files to keep (older files will be deleted)
LOG_FILE_LIMIT=20

# Streaming configuration
# Set to 'true' to enable streaming responses (default is false)
STREAM=false

# Default account to use first (if available)
# Set this to specify which account the proxy should use by default
# Should match the name used when adding an account with 'npm run auth add <name>'
DEFAULT_ACCOUNT=